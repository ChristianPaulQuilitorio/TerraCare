{
  "name": "terracare-angular",
  "version": "0.0.0",
  "description": "TerraCare â€” community app for tracking eco-challenges, resources, and forum.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChristianPaulQuilitorio/TerraCare.git"
  },
  "author": "202310339@gordoncollege.edu.ph",
  "license": "MIT",
  "keywords": [
    "teracare",
    "environment",
    "angular",
    "sustainability",
    "challenges"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "ng": "ng",
    "start": "cross-env NODE_NO_WARNINGS=1 ng serve --proxy-config proxy.conf.json",
    "start:dev-client": "npm run dev",
    "serve:ssr": "cross-env NODE_NO_WARNINGS=1 ng run terracare-angular:build:development && node dist/terracare-angular/server/server.mjs",
    "build:ssr": "cross-env NODE_NO_WARNINGS=1 ng run terracare-angular:build:production",
    "build": "cross-env NODE_NO_WARNINGS=1 ng build --configuration production",
    "build:dev": "cross-env NODE_NO_WARNINGS=1 ng build --configuration development",
    "build:ci": "cross-env NODE_NO_WARNINGS=1 ng build --configuration production",
    "vercel-build": "npm run build:ci && npm run bundle:server",
    "bundle:server": "node scripts/bundle-server.js && echo '\n[vercel-bundle-debug] api folder listing:' && node -e \"try{const fs=require('fs'); (fs.readdirSync('api')||[]).forEach(f=>console.log(f));}catch(e){console.error('[vercel-bundle-debug] error listing api:', e.message)}\" || true",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint . --ext .ts,.html --max-warnings=0",
    "serve:ssr:terracare-angular": "node dist/terracare-angular/server/server.mjs",
    "dev:server": "cross-env NODE_NO_WARNINGS=1 PORT=4000 node -r ts-node/register server.ts",
    "dev": "concurrently \"npm:dev:server\" \"npm:start\"",
    "dev:proxy": "cross-env PORT=4000 node -r dotenv/config server-dev-proxy.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/platform-server": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@angular/ssr": "^17.3.12",
    "@supabase/supabase-js": "^2.58.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "esbuild": "^0.18.17",
    "serverless-http": "^3.0.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "multer": "^1.4.5-lts.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.12",
    "@angular/cli": "^17.3.12",
    "@angular/compiler-cli": "^17.3.0",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^18.18.0",
    "concurrently": "^8.2.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.4.2"
  }

}
