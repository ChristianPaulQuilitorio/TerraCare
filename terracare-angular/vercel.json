{
  "version": 3,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/terracare-angular",
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "routes": [
    
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
